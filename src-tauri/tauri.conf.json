{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:3002",
    "distDir": "../dist"
  },
  "package": {
    "productName": "VJ.Tools",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.vj.tools",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "updater": {
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "VJ.Tools",
        "width": 1200,
        "height": 800
      }
    ]
  }
} 